#include <stdio.h>
// Funcion principal
int main()
{
 int i, j;
 for(i=1; i<=10; i++)
 {
 printf("\nTabla del %i\n", i);
 // El error se encontraba aqui debiamos de cambiar el == por <= para que el ciclo se completara 
 for(j=1; j<=10; j++)
 {
 printf("%i X %i = %i\n", i, j, i*j);
 }
 }
}
