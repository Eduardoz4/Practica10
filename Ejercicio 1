#include <stdio.h>
// FUncion principal 
int main () 
{
 int N, CONT, AS;
 AS=0;
 // Establece el valor del contador 
 CONT=1;
 printf("TECLEA UN NUMERO: ");
 scanf("%i",&N);
 while(CONT<=N)
 {
 AS=(AS+CONT);
 CONT=(CONT+2);
 }
 //Aqui le aÃ±adi una instruccion y un punto de ruptura para analizar el comportamiento del contador y entender el funcionamiento del codigo 
 printf("%i", CONT);
 printf("\nEL RESULTADO ES %i\n", AS);
}
